{
  "name": "joind.in 日本語化",
  "version": "0.7",
  "manifest_version": 2,
  "description": "joind.in のサイトを日本語化します。",
  "icons": {
    "16":  "joind.in.icon.png",
    "48":  "joind.in.icon.png",
    "128": "joind.in.icon.png"
  },
  "content_scripts": [
    {
       "matches": [
          "*://joind.in/*"
       ]
      ,"js": ["jquery-2.1.4.min.js", "content_script.js"]
    }
  ],
  "permissions": [
     "*://joind.in/*"
  ]
}